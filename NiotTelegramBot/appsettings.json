{
  "Token": "",
  // Input channels for user notifications
  "PluginsOutgoingInput": [
    {
      "Name": "Text",
      "Enabled": true,
      "InputDir": "./data/input",
      "OutputDir": "./data/output",
      "Options": "*.txt;*.log"
    },
    {
      "Name": "Photo",
      "Enabled": true,
      "InputDir": "./data/input",
      "OutputDir": "./data/output",
      "Options": "*.png;*.jpg"
    },
    {
      "Name": "MQTT",
      "Enabled": false,
      "Options": "none"
    }
  ],
  "PluginsDataSource": [
    {
      "Name": "InfluxDbLegacy", // InfluxDB 1.7+
      "Enabled": true,
      "Hostname": "localhost",
      "Port": 8086,
      "Proto": "HTTP",
      "DatabaseName": "", // Bucket
      "Username": "",
      "Password": "",
      "Timeout": 5
    },
    {
      "Name": "InfluxDb", // InfluxDB 2.x
      "Enabled": false,
      "Hostname": "localhost",
      "Port": 8086,
      "Proto": "HTTP", // http or https
      "DatabaseName": "", // Bucket
      "Username": "", // Organization
      "Password": "", // In case of InfluxDB it's a token
      "Timeout": 5
    }
  ],
  "PluginsProcessor": [
    {
      "Name": "Telegraf",
      "Enabled": true,
      "Options": "cpu;disk;mem;swap;system",
      "Order": 0
    },
    {
      "Name": "StartStopInfo",
      "Enabled": true,
      "Options": "",
      "Order": 1
    },
    {
      "Name": "Docker",
      "Enabled": true,
      "Options": "unix:///var/run/docker.sock", // Or npipe://./pipe/docker_engine on Windows
      "Order": 2
    }
  ],
  // Here you can add accepted users by username
  // To register Telegram username please follow Telegram help instructions: https://telegram.org/faq?setln=uz#q-what-are-usernames-how-do-i-get-one
  "AllowedUsernames": "test1;test2",
  // WARNING: Don't change this section
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Warning"
    }
  }
}